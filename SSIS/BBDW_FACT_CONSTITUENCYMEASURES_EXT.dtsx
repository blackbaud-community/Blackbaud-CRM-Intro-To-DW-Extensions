                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with CONSTITUENCYRANKINGS(MOSTRECENTDATERANK, CONSTITUENTDIMID, CONSTITUENCYDIMID) as
(
  select
    -- rank the most recent constituency "FROMDATE" value for each constituent
    row_number() over (partition by fc.CONSTITUENTDIMID order by fc.FROMDATE desc) as MOSTRECENTFROMDATE,
    fc.CONSTITUENTDIMID,
    fc.CONSTITUENCYDIMID

  from bbdw.FACT_CONSTITUENCY fc 
  join bbdw.DIM_CONSTITUENCY dc on dc.CONSTITUENCYDIMID = FC.CONSTITUENCYDIMID
)

select
  CONSTITUENTDIMID,
  CONSTITUENCYDIMID as MOSTRECENTCONSTITUENCYDIMID
from CONSTITUENCYRANKINGS
where MOSTRECENTDATERANK = 1 -- retrieve the most recent constituency for each constituent</property>
