                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- find the most recent constituency for each constituent
;with CONSTITUENCYRANKINGS as
(
  select
      -- rank the most recent constituency "FROMDATE" value for each constituent
    row_number() over (partition by fc.CONSTITUENTSYSTEMID order by fc.FROMDATEDIMID desc) as MOSTRECENTDATERANK,      
    fc.CONSTITUENTSYSTEMID, 
    dc.CONSTITUENCY

  from bbdw.FACT_CONSTITUENCY fc 
  join bbdw.DIM_CONSTITUENCY dc on dc.CONSTITUENCYDIMID = FC.CONSTITUENCYDIMID
)
select 
  CONSTITUENTSYSTEMID, 
  CONSTITUENCY as MOSTRECENTCONSTITUENCY
from CONSTITUENCYRANKINGS
where MOSTRECENTDATERANK = 1 -- retrieve the most recent constituency for each constituent</property>
